<% form_for(@chemical) do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label "Storage Location" %><br />
    <%= f.text_field :storage_loc %>
  </p>
  <p>
    <%= f.label "Storage Temperature" %><br />
    <%= f.text_field :storage_temp %>
  </p>
  <p>
    <%= f.label :company %><br />
    <%= f.text_field :company %>
  </p>
  <p>
    <%= f.label  "Product Number" %><br />
    <%= f.text_field :cat_num %>
  </p>
  <p>
    <%= f.label :price %><br />
    <%= f.text_field :price %>
  </p>
  <p>
    <%= f.label :unit %><br />
    <%= f.text_field :unit %>
  </p>
  <p>
    <%= f.label :notes %><br />
    <%= f.text_area :notes %>
  </p>
  <p>
    <%= f.submit action %>
  </p>
<% end %>
